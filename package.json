  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "db:init": "supabase init",
    "db:link": "supabase link --project-ref your-project-ref",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset",
    "db:generate": "supabase gen types typescript --local > types/supabase/database.types.ts",
    "db:migrate": "supabase migration new",
    "db:status": "supabase db remote commit",
    "db:setup": "npm run db:setup:schema && npm run db:setup:rls",
    "db:setup:schema": "node -e \"const {createClient} = require('@supabase/supabase-js'); const fs = require('fs'); const supabase = createClient(process.env.NEXT_PUBLIC_SUPABASE_URL, process.env.NEXT_PUBLIC_SUPABASE_ANON_KEY); const sql = fs.readFileSync('scripts/supabase_schema.sql', 'utf-8'); console.log('SQL 파일을 Supabase Dashboard의 SQL Editor에서 실행하세요.');\"",
    "db:setup:rls": "node -e \"console.log('RLS 파일을 Supabase Dashboard의 SQL Editor에서 실행하세요.');\"",
    "db:test": "tsx scripts/test-schema.ts",
    "check:naming": "tsx scripts/check-naming-convention.ts"
  },
